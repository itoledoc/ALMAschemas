<Type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="Alma/ACSError"
    xsi:schemaLocation="Alma/ACSError ACSError.xsd"
    name="SchedulingExceptions"
    type="80000"
    _prefix="alma">

<ErrorCode name="GeneralSchedulingException"
           shortDescription="General Scheduling Error."
           description="There was a problem within the scheduling subsystem."/>
<ErrorCode name="InvalidOperation"
           shortDescription="Invalid operation requested."
           description="The request is invalid for the current mode and state."/>
<ErrorCode name="UnidentifiedResponse"
           shortDescription="Unidentifiable message."
           description="A response was received that contained an unidentifiable message identifier." />
<ErrorCode name="InvalidObject"
           shortDescription="Object not valid."
           description="One or more objects are not valid. " />
<ErrorCode name="SBExists"
           shortDescription="SB Exists."
           description="The SB already exists." />
<ErrorCode name="NoSuchSB"
           shortDescription="No such SB."
           description="There is no such SB." />
<ErrorCode name="CannotRunCompleteSB"
           shortDescription="SB is complete."
           description="SB is complete and cannot be run anymore." />
<ErrorCode name="SBNotRunnable"
           shortDescription="The selected SB is in incorrect status"
           description="The Scheduler can only run SchedBlocks which are in a suitable state." />
<ErrorCode name="ObsProjectRejected"
           shortDescription="ObsProject has been rejected"
           description="Scheduling has discarded ObsProject." />
<ErrorCode name="SchedBlockRejected"
           shortDescription="SchedBlock has been rejected"
           description="Scheduling has discarded SchedBlock." />
</Type>    

